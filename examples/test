Forward(Init){
	*(Init): MyDir(E) & True ? Move(F) : (Init)
	| True ? Turn(R) : (Init)
}

Virus(Init){

    *(Init):
    |Key(o) ? Move(N) :(Init)

    |Key(m) ? Move(E) :(Init)

    |Key(l)? Move(S) :(Init)

    |Key(k) ? Move(O) :(Init)

    |Key(j) ? Pick() :(Init)
    |Key(n) ? Get() :(Init)
    |Key(p) ? Pop() :(Init)
    |Key(i) ? Wizz() :(Init)
    |Key(y) ? Hit() :(Init)
    |True ? Jump() :(Init)
    
}

Antivirus(Init){

    *(Init):
    |Key(z) ? Move(N) :(Init)
    |Key(s)? Move(S) :(Init)
    |Key(q) ? Move(O) :(Init)
    |Key(f) ? Move(E) :(Init)

    |Key(g) ? Pick() :(Init)
    |Key(v) ? Get() :(Init)
    |Key(r) ? Pop() :(Init)
    |Key(a) ? Wizz() :(Init)
    |Key(c) ? Hit() :(Init)
    |True ? Jump() :(Init)

}

ZigAg(Init){

	*(Init): True ? Move(L) :(Left1)
	*(Left1): True ? Move(F) :(Front1)
	*(Front1): True ? Move(R) :(Right1)
	*(Right1): True ? Move(R) :(Right2)
	*(Right2): True ? Move(F) :(Front2)
	*(Front2): True ? Move(L) :(Init)
}
